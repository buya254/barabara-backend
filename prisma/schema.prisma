generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  INSPECTOR
  SITE_AGENT
  RE
  ARE
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  role     Role
}
